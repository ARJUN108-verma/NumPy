import numpy as np

# 1. Create a 1D array
array_1d = np.array([10, 20, 30, 40, 50])
print("1D Array:")
print(array_1d)

# 2. Create a 2D array
array_2d = np.array([[1, 2, 3], [4, 5, 6]])
print("\n2D Array:")
print(array_2d)

# 3. Create an array of zeros
zeros_array = np.zeros((2, 3))
print("\nArray of Zeros:")
print(zeros_array)

# 4. Create an array of ones
ones_array = np.ones((3, 2))
print("\nArray of Ones:")
print(ones_array)

# 5. Create an array with a range of values
range_array = np.arange(0, 10, 2)
print("\nArray with range 0 to 10 with step 2:")
print(range_array)

# 6. Create an identity matrix
identity_matrix = np.eye(3)
print("\nIdentity Matrix (3x3):")
print(identity_matrix)
